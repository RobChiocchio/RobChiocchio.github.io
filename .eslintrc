{
    "extends": ["wesbos"],
    "rules": {
      "no-console": 2,
      "import/no-unresolved": 0,
      "react/display-name": 0,
      "react/prop-types": 0,
      "quotes": [1, "double"],
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "es5",
          "singleQuote": false,
          "printWidth": 120,
          "tabWidth": 4,
          "semi": true
        }
      ]
    }
  }
  